<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.activebartest"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.activebartest.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.activebartest.SearchActivity"
            android:label="@string/title_activity_results" >
        </activity>
        <activity
            android:name="com.example.activebartest.LanguageOptions"
            android:label="@string/title_activity_language_options" >
        </activity>
        <activity
            android:name="com.example.activebartest.HistoryActivity"
            android:label="@string/title_activity_history" >
        </activity>
    </application>

</manifest>